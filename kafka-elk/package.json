{
  "name": "ts-node-kafka-express",
  "version": "1.0.0",
  "description": "Nodejs, TypeScript, Kafkajs and Express",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist & tsc --project tsconfig.json && tscpaths -p tsconfig.json -s src -o dist",
    "start": "node dist/app.js",
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "lint": "eslint --fix --ext .ts src/",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "nodejs",
    "kafkajs"
  ],
  "author": "williamkoller <williamkoller30@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "ip": "^2.0.1",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "winston": "^3.8.2",
    "kafkajs": "^2.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/ip": "^1.1.3",
    "@types/kafkajs": "^1.9.0",
    "@types/morgan": "^1.9.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^3.1.0",
    "prettier": "^2.8.7",
    "sucrase": "^3.32.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.4",
    "typescript-transform-paths": "^3.4.7"
  },
  "engines": {
    "node": "18"
  }
}
